#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    long long int n;
    cin>>n;
    long long int num,count=0;
    for(long long int i=0;i<n;i++)
    {
        cin>>num;
        while(num>0)
        {
            if(num%2==1) count++;
            num/=2;
        }
    }
    cout<<count;
    return 0;
}
